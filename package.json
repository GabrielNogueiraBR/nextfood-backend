  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install .config/husky",
    "build": "nest build",
    "dev": "tsnd --exit-child --rs --watch *.env --inspect=0.0.0.0:9229 ./source/main.ts",
    "start": "node ./source/main.js || (cd dist && node ./source/main.js)",
    "docker": "pnpm run build && docker build -t template-nestjs dist && docker run --rm -it -p 8080:8080 template-nestjs",
    "lint": "export TIMING=1 || set TIMING=1&& eslint **/*.ts --max-warnings=0",
    "test": "jest --verbose --forceExit --passWithNoTests --testTimeout=30000 --config .config/jest/.jestrc.json"
  },
  "dependencies": {
    "@nestjs/common": "8.4.0",
    "@nestjs/core": "8.4.0",
    "@nestjs/platform-express": "8.4.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.4"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "rimraf": "3.0.2",
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@nestjs/cli": "8.2.2",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "commitlint": "16.2.1",
    "eslint": "8.10.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.5",
    "ts-jest": "27.1.3",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.2"
  }
}
